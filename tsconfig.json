{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "noImplicitReturns" : true,
    "noFallthroughCasesInSwitch" : true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": true,
    "experimentalDecorators": true,
    "declaration": true,
    "sourceMap": true,
    "outDir": "./dist"
  },
  "exclude": [
    "node_modules"
  ]
}
